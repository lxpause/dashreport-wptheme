			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "2X petit",
					"size": "8px",
					"slug": "xx-small"
				},
				{
					"name": "Très petit",
					"size": "clamp(0.75rem, 0.659rem + 0.45vw, 1rem)",
