				},
				{
					"name": "2X grand",
					"size": "clamp(3.688rem, 2.847rem + 4.2vw, 6rem)",
					"slug": "xx-large"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/Inter-Variable.woff2"
							]
						}
					],
					"fontFamily": "Inter, sans-serif",
					"name": "Inter",
					"slug": "inter"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Figtree",
